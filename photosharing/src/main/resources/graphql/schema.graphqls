type Query {
    allUsers: [User]
    allPhotos: [Photo]
    userByUsername(username: String!): User
}

type User {
    id: ID!
    username: String!
    photos: [String]
    likedPhotos: [String]
}

type Photo {
    id: ID!
    title: String!
    description: String
    uploadUser: String
    imgBase64: String
}